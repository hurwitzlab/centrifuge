{
  "name":"centrifuge-0.0.6-test",
  "appId": "centrifuge-0.0.6",
  "batchQueue": "normal",
  "executionSystem": "tacc-stampede2-kyclark",
  "maxRunTime": "12:00:00",
  "memoryPerNode": "32GB",
  "nodeCount": 1,
  "processorsPerNode": 16,
  "archive": true,
  "archiveSystem": "data.iplantcollaborative.org",
  "archivePath": null,
  "inputs": {
    "QUERY": [ 
      "/kyclark/data/dolphin/fasta/Dolphin_1_z04.fa",
      "/kyclark/data/dolphin/fasta/Dolphin_2_z09.fa"
    ]
  },
  "parameters": {
    "INDEX": "p+h+v",
    "EXCLUDE_TAXIDS": "9606",
    "FORMAT": "fasta"
  },
  "notifications": [
    {
      "url":"https://requestbin.agaveapi.co/1if1nnc1?job_id=${JOB_ID}&status=${JOB_STATUS}",
      "event":"*",
      "persistent":true
    },
    {
      "url":"kyclark@gmail.com",
      "event":"FINISHED",
          "persistent":false
    },
    {
      "url":"kyclark@gmail.com",
      "event":"FAILED",
      "persistent":false
    }
  ]
}
